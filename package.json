{
  "name": "pkg-actions",
  "version": "1.0.0",
  "author": "Sridhar Mallela",
  "license": "MIT",
  "description": "test",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sridharmallela/pkg-actions.git"
  },
  "keywords": [],
  "scripts": {
    "build": "npm run check-versions && npm run clean && npm run build-unit && npm run print-banner",
    "build-unit": "tsc -p \"./tsconfig.json\"",
    "check-versions": "versions-checker --node \">=22\" --npm \">=10\"",
    "clean": "rimraf \"./dist\" \"./coverage\"",
    "prepare": "is-ci || husky",
    "print-banner": "print-cli -b -c \"green\" \"pkg-actions\"",
    "release:build": "npm run check-versions && npm run clean && npm run build-unit && npm run print-banner && npm start",
    "start": "node \"./dist/index.js\"",
    "test": "jest --config \"./jest.config.ts\"",
    "validate": "npm run check-versions && npm run clean && npm test && npm run build-unit && npm run print-banner"
  },
  "devDependencies": {
    "@commitlint/cli": "~19.6.1",
    "@types/jest": "~29.5.14",
    "@types/node": "~22.10.10",
    "commitlint-config-ngs": "~1.0.2",
    "cross-env": "~7.0.3",
    "dotenv": "~16.4.7",
    "eslint-config-ngs": "~1.0.1",
    "gaxios": "~6.7.1",
    "husky": "~9.1.7",
    "is-ci": "~4.1.0",
    "jest": "~29.7.0",
    "jest-environment-node": "~29.7.0",
    "jest-preset-ngs": "~1.0.5",
    "lint-staged": "~15.4.3",
    "npm-commands-ngs": "~1.0.1",
    "nx-commands-ngs": "~1.0.1",
    "prettier": "~3.4.2",
    "prettier-config-ngs": "~1.0.3",
    "print-cli": "~2.1.12",
    "rimraf": "~6.0.1",
    "stylelint-config-ngs": "~1.0.1",
    "ts-jest": "~29.2.5",
    "ts-node": "~10.9.2",
    "tslib": "~2.8.1",
    "typescript": "~5.7.3",
    "versions-checker": "~2.1.12"
  }
}
